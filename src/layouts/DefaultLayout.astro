---
import Meta from "../components/Meta.astro";
import Footer from "../components/Footer.astro";

const { title } = Astro.props;
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <Meta title={title} />
  </head>
  <body>
    <slot />
    <Footer />
  </body>
</html>
